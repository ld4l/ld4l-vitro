<?xml version="1.0" encoding="UTF-8"?>

<rdf:RDF 
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" 
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xml:base="http://ld4l.org/ontology/core#">

    <!-- LD4L properties -->
    
    <owl:ObjectProperty rdf:about="http://ld4l.org/ontology/core#hasAlternateName">
        <rdfs:label>alternate Name</rdfs:label>
        <rdfs:comment>Relates a thing to its pseudonym or other alternative name.</rdfs:comment>
        <rdfs:range rdf:resource="http://www.w3.org/2006/vcard/ns#VCard" />
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://ld4l.org/ontology/core#alternateNameFor">
        <rdfs:label>alternate name for</rdfs:label>
        <rdfs:comment>Relates a pseudonym or other alternate name to the thing it refers to. </rdfs:comment>
        <owl:inverseOf rdfs:resource="http://ld4l.org/ontology/core#hasAlternateName" />        
    </owl:ObjectProperty>
    
</rdf:RDF>